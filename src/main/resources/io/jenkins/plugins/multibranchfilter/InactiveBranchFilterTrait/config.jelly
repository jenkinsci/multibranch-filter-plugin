    <f:number default="30" min="0"/>
  </f:entry>
  <f:entry title="${%Allow list}" field="allowlist">
    <f:textarea default="master&#10;main"/>
  </f:entry>
  <f:entry title="${%Deny list}" field="denylist">
    <f:textarea default=""/>
  </f:entry>
</j:jelly>
